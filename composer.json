{
  "name": "nickaguilarh/holo-settings",
  "description": "A package that allow entities to have settings stored in table.",
  "type": "library",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Nick Aguilar H",
      "email": "nickaguilarh@gmail.com"
    }
  ],
  "require": {
    "php": "*",
    "laravel/framework": "*",
    "ramsey/uuid": "*",
    "nickaguilarh/holo-constraints": "*"
  },
  "autoload": {
    "psr-4": {
      "Holo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Holo\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Holo\\HoloServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
